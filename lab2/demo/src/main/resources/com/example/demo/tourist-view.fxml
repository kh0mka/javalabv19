<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="290.0" prefWidth="371.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="com.example.demo.TouristController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label alignment="TOP_CENTER" blendMode="HARD_LIGHT" prefHeight="18.0" prefWidth="134.0" text="Tourists Center" textOverrun="LEADING_WORD_ELLIPSIS">
      <font>
         <Font size="17.0" />
      </font>
   </Label>
    <Button fx:id="userButton" onAction="#onUserButtonClick" prefHeight="39.0" prefWidth="163.0" text="User" />
    <Button fx:id="adminButton" mnemonicParsing="false" onAction="#onAdminButtonClick" prefHeight="40.0" prefWidth="160.0" text="Admin" />

    <Label fx:id="welcomeText" />
</VBox>
